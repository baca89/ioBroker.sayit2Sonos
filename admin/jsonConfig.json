{
	"i18n": true,
	"type": "panel",
	"items": {
		"accessKey": {
			"type": "text",
			"label": "Access Key",
			"newLine": true
		},
		"secretKey": {
			"type": "text",
			"label": "Secret-Key",
			"newLine": true
		},
		"announce": {
			"type": "checkbox",
			"label": "use Announcement",
			"newLine": true
		},
		"annoDuration": {
			"type": "number",
			"label": "Announcement Duration",
			"min": 0,
			"max": 30,
			"newLine": false
		},
		"annoTimeout": {
			"type": "number",
			"min": 3,
			"max": 30,
			"label": "Timeout for Announcement",
			"newLine": false
		},
		"annoVolume": {
			"type": "number",
			"min": 0,
			"max": 100,
			"label": "use Announcement",
			"newLine": true
		},
		"cache": {
			"newLine": true,
			"type": "checkbox",
			"label": "Cache",
			"help": "Cache generated files",
			"sm": 6,
			"md": 4,
			"lg": 2
		},
		"cacheExpiryDays": {
			"type": "number",
			"min": 0,
			"max": 1000,
			"label": "Cache-Expiry",
			"help": "in days",
			"hidden": "!data.cache",
			"sm": 6,
			"md": 4,
			"lg": 2
		},
		"webInstance": {
			"newLine": true,
			"type": "instance",
			"adapter": "web",
			"label": "Web instance",
			"sm": 6,
			"md": 4,
			"lg": 2
		},
		"webServer": {
			"type": "ip",
			"label": "Web server IP",
			"sm": 6,
			"md": 4,
			"lg": 2
		}
	}
